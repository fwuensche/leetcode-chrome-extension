{
  "name": "LeetCode TA",
  "description" : "Add button with link to YouTube solutions for every LeetCode problem (for now)",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "search.html",
    "default_icon": "img/lc-logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": [
        "jquery.js",
        "script.js"
      ]
    }
  ],
  "manifest_version": 2
}
